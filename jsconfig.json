{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@root/*": [
        "./*"
      ],
      "@./src/*": [
        "./src/*"
      ],
      "@app/*": [
        "./src/app/*"
      ],
      "@cast/*": [
        "./src/app/cast/*"
      ],
      "@controller/*": [
        "./src/app/controller/*"
      ],
      "@helper/*": [
        "./src/app/helper/*"
      ],
      "@middleware/*": [
        "./src/app/middleware/*"
      ],
      "@model/*": [
        "./src/app/model/*"
      ],
      "@service/*": [
        "./src/app/service/*"
      ],
      "@config/*": [
        "./src/config/*"
      ],
      "@evevnt/*": [
        "./src/evevnt/*"
      ],
      "@job/*": [
        "./src/job/*"
      ],
      "@resources/*": [
        "./src/resources/*"
      ],
      "@schedule/*": [
        "./src/schedule/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
    }
  }
}